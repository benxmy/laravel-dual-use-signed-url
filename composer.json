{
    "name": "benxmy/laravel-limited-use-signed-url",
    "description": "Laravel Limited Use Signed URL",
    "keywords": [
        "benxmy",
        "laravel-limited-use-signed-url"
    ],
    "homepage": "https://github.com/benxmy/laravel-limited-use-signed-url",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ben",
            "email": "benxmy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Benxmy\\LimitedUseSignedUrl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Benxmy\\LimitedUseSignedUrl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Benxmy\\LimitedUseSignedUrl\\LaravelLimitedUseSignedUrlServiceProvider"
            ],
            "aliases": {
                "LimitedUseSignedUrl": "Benxmy\\LimitedUseSignedUrl\\LaravelLimitedUseSignedUrlFacade"
            }
        }
    }
}
